<div class="container">
  <div class="header-fixed">
    <h1><i class="fas fa-paw"></i> Meet Our Partners</h1>
    <table>
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Address</th>
          <th scope="col">Email</th>
          <th scope="col">Phone</th>
          <th scope="col">Action</th>
        </tr>
      </thead>
      <tbody>
        <% @partners.each do |partner| %>
          <tr>
            <td><%= partner.name %></td>
            <td><%= partner.address %></td>
            <td><%= partner.email %></td>
            <td><%= partner.phone %></td>
            <td><button><%= link_to 'View', partner_path(partner) %></button></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
