<div class="container">
  <div class="row">
    <div class="col">
      <h1>Edit Partner</h1>
      <%= simple_form_for [@partner] do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :email %>
        <%= f.input :phone %>
        <%= f.submit %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
